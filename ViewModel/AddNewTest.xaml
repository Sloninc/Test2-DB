<Window x:Class="Test2.ViewModel.AddNewTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test2.ViewModel"
        mc:Ignorable="d"
        Title="AddNewTest" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Vertical"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <TextBlock Text="TestDate"/>
            <TextBox Text=""/>
            <TextBlock Text="BlockName"/>
            <TextBox Text=""/>
            <TextBlock Text="Note"/>
            <TextBox Text=""/>
        </StackPanel>
        <Button Grid.Row="1" Content="Add test"/>
    </Grid>
</Window>
